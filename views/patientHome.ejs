          <!-- ============================================================== -->
          <div class="row">
                <div class="col-md-6 col-sm-6 col-lg-6 col-xl-6">
                        <a href="/patient/History">
                        <div class="dash-widget">
              <span class="dash-widget-bg1"><i class="fa fa-stethoscope" aria-hidden="true"></i></span>
              <div class="dash-widget-info text-right">
            <h5>Click Here To view Prescription</h5>
                <span class="widget-title1">Prescription<i class="fa fa-check" aria-hidden="true"></i></span>
              </div>
                        </div>
                        </a>
                    </div>
    
                <div class="col-md-6 col-sm-6 col-lg-6 col-xl-6">
                    <a href="/patient/profile">
                        <div class="dash-widget">
                            <span class="dash-widget-bg2"><i class="fa fa-user-o"></i></span>
                            <div class="dash-widget-info text-right">
                                    <h5>Click here to update Profile</h5>
                                <span class="widget-title2">Profile <i class="fa fa-check" aria-hidden="true"></i></span> 
                            </div>
                        </div>
                      </a>
                  
                    </div>
        </div>
        <h3 style="text-align: center;">Patient</h3>

        <div class="row">
          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <table id="example" class=" table table-striped table-bordered">
              <thead>
                <tr>
                  <th>Date</th>
                  <th>Patient Email</th>
                  <th>Doctor</th>
                  <th>Status</th>
  
  
  
  
                </tr>
              </thead>
  
  
              <div class="row">
  
  
                <div class="table-responsive">
  
                  <% for(let i = 0; i < posts.length; i++) { %>
                  <tbody>
                    <tr>
                      <td><%= posts[i].date %></td>
                      <td><%= posts[i].patient %></td>
                      <td><%= posts[i].dr %></td>
                      <td><%= posts[i].status %></td>
                    </tr>
                  </tbody>
  
                  <% } %>
  
                </div>
              </div>
          </div>
  
  
          </table>
        </div>
              </div>
              </div>
              </div>


    
